{
  "name": "ceria-web-app",
  "version": "0.1.0",
  "homepage": "http://digibank-ceria.github.io/iframe-ceria",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "winBuild": "set \"GENERATE_SOURCEMAP=false\" && react-scripts build",
    "build:prod": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm run test -- --coverage --watchAll=true",
    "test:coverages": "npm run test -- --bail --ci --coverage --reporters=default --reporters=jest-junit --runInBand",
    "win-start": "set HTTPS=true&&react-script start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
